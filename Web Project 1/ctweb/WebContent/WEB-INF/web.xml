<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ctweb</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/LoginUrl</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>saveemp</servlet-name>
    <servlet-class>controller.EmployeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>saveemp</servlet-name>
    <url-pattern>/saveEmpUrl</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>delUpdate</servlet-name>
    <servlet-class>controller.EmployeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>delUpdate</servlet-name>
    <url-pattern>/actUrl</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>updateEmp</servlet-name>
    <servlet-class>controller.UpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>updateEmp</servlet-name>
    <url-pattern>/updateEmpUrl</url-pattern>
  </servlet-mapping>
  
  
</web-app>